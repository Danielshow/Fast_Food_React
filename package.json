{
  "name": "fast_tfood",
  "version": "1.0.0",
  "description": "A web application for ordering food",
  "main": "index.jsx",
  "repository": "https://github.com/Danielshow/Fast_Food_React.git",
  "author": "Daniel Shotonwa <danielshotonwa53@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --open",
    "start": "npm run build && webpack && node dist/index.js",
    "build": "rm -rf dist/ && babel ./server --out-dir dist/ --ignore ./node_modules",
    "lint": "./node_modules/eslint/bin/eslint.js './**/*.jsx'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors && cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.1",
    "express": "^4.16.4",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "moxios": "^0.4.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-pager": "^1.3.3",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-toast-notifications": "^1.3.1",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2"
  },
  "devDependencies": {
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "webpack-dev-server": "^3.1.11",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4"
  }
